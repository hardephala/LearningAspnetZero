            </div>

            <div class="my-3">
                <label class="form-label" for="Cargorelease_agentcode">@L("agentcode")</label>
                <input class="form-control" id="Cargorelease_agentcode" value="@Model.Cargorelease.agentcode" type="text" name="agentcode" maxlength="@Arch.Cargoreleases.CargoreleaseConsts.MaxagentcodeLength" minlength="@Arch.Cargoreleases.CargoreleaseConsts.MinagentcodeLength" />
