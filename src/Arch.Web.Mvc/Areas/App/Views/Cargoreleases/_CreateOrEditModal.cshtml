
@using System.Globalization
@using Arch.Web.Areas.App.Models.Common.Modals
@using Arch.Web.Areas.App.Models.Cargoreleases
@model CreateOrEditCargoreleaseModalViewModel
@using Abp.Extensions
@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(Model.IsEditMode ? (L("EditCargorelease")) : L("CreateNewCargorelease")))

<div class="modal-body">
    <div id="CargoreleaseInformationsTab">
        <form name="CargoreleaseInformationsForm" role="form" novalidate class="form-validation">

            @if (Model.IsEditMode)
            {
            <input type="hidden" name="id" value="@Model.Cargorelease.Id" />
            }

            <div class="my-3">
                <label class="form-label" for="Cargorelease_priority">@L("priority")</label>
                <input class="form-control" id="Cargorelease_priority" value="@Model.Cargorelease.priority" type="text" name="priority" maxlength="@Arch.Cargoreleases.CargoreleaseConsts.MaxpriorityLength" minlength="@Arch.Cargoreleases.CargoreleaseConsts.MinpriorityLength" />
            </div>

            <div class="my-3">
                <label class="form-label" for="Cargorelease_blno">@L("blno")</label>
                <input class="form-control" id="Cargorelease_blno" value="@Model.Cargorelease.blno" type="text" name="blno" maxlength="@Arch.Cargoreleases.CargoreleaseConsts.MaxblnoLength" minlength="@Arch.Cargoreleases.CargoreleaseConsts.MinblnoLength" />
            </div>

            <div class="my-3">
                <label class="form-label" for="Cargorelease_invoicevalidity">@L("invoicevalidity")</label>
