                <label class="form-label" for="Cargorelease_entrydate">@L("entrydate")</label>
                <input class="form-control m-input date-picker" id="Cargorelease_entrydate" type="text" name="entrydate" value="@Model.Cargorelease.entrydate" />
            </div>

            <div class="my-3">
