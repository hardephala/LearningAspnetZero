            </div>

            <div class="my-3">
                <label class="form-label" for="Cargorelease_ipaddr">@L("ipaddr")</label>
                <input class="form-control" id="Cargorelease_ipaddr" value="@Model.Cargorelease.ipaddr" type="text" name="ipaddr" maxlength="@Arch.Cargoreleases.CargoreleaseConsts.MaxipaddrLength" minlength="@Arch.Cargoreleases.CargoreleaseConsts.MinipaddrLength" />
            </div>

        </form>
    </div>
</div>

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalFooterWithSaveAndCancel.cshtml")