                <input class="form-control" id="Cargorelease_deliveryorderno" value="@Model.Cargorelease.deliveryorderno" type="text" name="deliveryorderno" maxlength="@Arch.Cargoreleases.CargoreleaseConsts.MaxdeliveryordernoLength" minlength="@Arch.Cargoreleases.CargoreleaseConsts.MindeliveryordernoLength" />
            </div>

            <div class="my-3">
                <label class="form-label" for="Cargorelease_customercode">@L("customercode")</label>
